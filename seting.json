{
    "workbench.colorTheme": "Learn with Sumit - Peace of the eye - Dracula version",
    "editor.mouseWheelZoom": true,
    "tabnine.experimentalAutoImports": true,
    "workbench.iconTheme": "vscode-icons",
    "editor.fontSize": 20,
    "editor.wordWrap": "on",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.cursorStyle": "line",
    "editor.minimap.maxColumn": 30,
    "editor.minimap.autohide": true,
    "git.autofetch": true,
    "tailwindCSS.emmetCompletions": true,
    "tailwindCSS.includeLanguages": {
        "plaintext": "html"
    },
    "files.associations": {
        "*.css": "tailwindcss"
      },
    "editor.quickSuggestions": {
        "strings": true
      },

    "liveSassCompile.settings.formats": [    
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": null,
            "savePathReplacementPairs": null
        },
        {
            "format": "compressed",
            "extensionName": ".min.css",
            "savePath": null,
            "savePathReplacementPairs": null
        }
    ],
    "files.autoSave": "onFocusChange",
    
    "diffEditor.ignoreTrimWhitespace": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "cSpell.userWords": [
        "daisyui",
        "hasanmajedul",
        "swal",
        "tailwindcss"
    ],
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.bracketSameLine": true,
    "prettier.jsxSingleQuote": true,
    "prettier.proseWrap": "always",
    "prettier.singleAttributePerLine": true,
    "prettier.singleQuote": true,
    "prettier.vueIndentScriptAndStyle": true,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "eslint.probe": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "vue",
        "markdown",
        "jsx",
        "react"
    ],
    "eslint.run": "onSave",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "css.lint.unknownVendorSpecificProperties": "warning",
    "less.lint.unknownVendorSpecificProperties": "warning",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontSize": 20,
    "terminal.integrated.lineHeight": 1.15,
    "editor.accessibilityPageSize": 20,
    "editor.codeLensFontSize": 10,
    "editor.inlayHints.fontSize": 10,
    "editor.suggestFontSize": 15,
    "scm.inputFontSize": 15,
    "debug.console.fontSize": 15,
    "notebook.markup.fontSize": 15,
    "notebook.output.fontSize": 15,
    "gitlens.gitCommands.search.showResultsInSideBar": false,
    "editor.guides.bracketPairs": true,
    "git.confirmSync": false,
    "bootstrapAutocomplete.version": "5.3",
    "sqltools.connections": [
        {
            "mysqlOptions": {
                "authProtocol": "default",
                "enableSsl": "Disabled"
            },
            "previewLimit": 50,
            "server": "localhost",
            "port": 3360,
            "driver": "MySQL",
            "name": "bidya",
            "database": "world",
            "username": "root",
            "askForPassword": true
        },
        {
            "previewLimit": 50,
            "server": "localhost",
            "port": 5432,
            "askForPassword": true,
            "driver": "PostgreSQL",
            "name": "postgres",
            "database": "postgres",
            "username": "postgres"
        }
    ],
    "accessibility.signals.terminalBell": {
        "sound": "on"
    },
    "terminal.integrated.enableVisualBell": true,
    "remote.SSH.remotePlatform": {
        "103.228.38.27": "linux"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "window.commandCenter": false
}
